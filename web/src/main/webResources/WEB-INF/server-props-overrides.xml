<!-- 
	common overrides for all geonetwork files   
-->
<overrides>
	<textFile name=".*/WEB-INF/server.prop">
		<update linePattern="buildProfile=geonetworkBuildProfile">buildProfile=${geonetworkBuildProfile}</update>
		<update linePattern="subVersion=geonetworkSubversion">subVersion=${geonetworkSubversion}</update>
		<update linePattern="version=geonetworkVersion">version=${geonetworkVersion}</update>
		<update linePattern="release=geonetworkVersion">release=${geonetworkVersion}</update>
		<update linePattern="javaVersion=java.version">javaVersion=${geonetworkVersion}</update>
		<update linePattern="OS=os">OS=${os.name} ${os.version} ${os.arch}</update>
	</textFile>
</overrides>